<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item
        android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient
                android:angle="270"
                android:centerY="0.75"
                android:endColor="#FFFFFF"
                android:startColor="#FFFFFF"
                />

        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners
                    android:radius="0dp"/>
                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#F39918"
                    android:startColor="#F39918"/>
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient
                    android:angle="270"
                    android:centerY="0.75"
                    android:endColor="#ed8d03"
                    android:startColor="#ed8d03"/>
            </shape>
        </clip>
    </item>
</layer-list>